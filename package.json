  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/bull": "^3.15.9",
    "axios": "^1.6.7",
    "body-parser": "^2.2.0",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "googleapis": "^150.0.1",
    "redis": "^5.5.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
