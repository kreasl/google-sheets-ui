    "generate-game-results": "ts-node src/scripts/generate-game-results.ts",
    "generate-users": "ts-node src/scripts/generate-users.ts",
    "generate-bets": "ts-node src/scripts/generate-bets.ts",
    "generate-results": "ts-node src/scripts/generate-results.ts",
    "sheets-to-csv": "ts-node src/scripts/google-sheets-to-csv.ts",
